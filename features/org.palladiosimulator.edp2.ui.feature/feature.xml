      <import plugin="org.eclipse.emf.edit"/>
      <import plugin="org.eclipse.emf.common.ui"/>
      <import plugin="org.eclipse.swt"/>
      <import plugin="org.eclipse.ui" version="3.105.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.metricspec" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.views.properties.tabbed" version="3.6.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime" version="3.9.100" match="greaterOrEqual"/>
      <import plugin="org.apache.commons.collections15" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.apache.batik.dom" version="1.6.0" match="greaterOrEqual"/>
      <import plugin="org.apache.batik.ext.awt" version="1.6.0" match="greaterOrEqual"/>
      <import plugin="org.apache.batik.svggen" version="1.6.0" match="greaterOrEqual"/>
