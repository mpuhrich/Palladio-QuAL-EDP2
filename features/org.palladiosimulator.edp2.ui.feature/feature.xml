      <import plugin="org.palladiosimulator.edp2.dao" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.util" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.palladiosimulator.edp2.visualization" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.visualization.jfreechart" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.repository.local" version="1.0.0" match="greaterOrEqual"/>
