<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.edp2.ui.feature"
      label="EDP2 UI Feature"
      version="1.0.1.qualifier"
      plugin="org.palladiosimulator.branding"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="https://sdqweb.ipd.kit.edu/wiki/PCM_Development/EDP2/Visualization">
