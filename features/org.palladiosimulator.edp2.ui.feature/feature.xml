      <import plugin="org.eclipse.core.databinding"/>
      <import plugin="org.eclipse.emf.databinding"/>
      <import plugin="org.eclipse.core.databinding.property"/>
      <import plugin="org.eclipse.emf.databinding.edit" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.jscience" version="4.1.3" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.databinding.beans" version="1.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.edit" version="1.0.0" match="greaterOrEqual"/>
