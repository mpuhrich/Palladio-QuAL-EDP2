         id="org.palladiosimulator.edp2.visualization.jfreechart"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.branding"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.edp2.batchexport"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
